    <style>
        button {
            border-radius: 15px;
            background: linear-gradient( #80ff72, #7ee8fa);
            height: 45px;
            width: 120px;
        }

        .sidenav {
            width: 130px;
            position: fixed;
            z-index: 1;
            top: 20px;
            left: 10px;
            background: linear-gradient( #80ff72, #7ee8fa);
            overflow-x: hidden;
            padding: 8px 0;
            height: 640px;
            border-radius: 21px;
            margin-top: -15px;
            overflow: hidden;
        }

            .sidenav a {
                padding: 6px 8px 6px 16px;
                text-decoration: none;
                font-size: 25px;
                color: #ffffff;
                display: block;
                margin-left: 25px;
            }

                .sidenav a:hover {
                    color: #064579;
                }

        .main {
            margin-left: 200px; /* Same width as the sidebar + left position in px */
            font-size: 28px; /* Increased text to enable scrolling */
            padding: 0px 10px;
            margin-top: -20px;
        }

        .current {
            width: 400px;
            position: fixed;
            background-image: url('../../MigoPlanter-ForestGreen_1024x1024.jpg');
            overflow-x: hidden;
            padding: 8px 0;
            height: 580px;
            border-radius: 21px;
            margin-left: 1050px;
            margin-top: -580px;
        }
    </style>
