    <h3 style="margin-left:510px; color:#064579;margin-top:40px;">Are you sure you want to delete this Project Result?</h3>
    <br />
    <div class="container mt-5 d-flex justify-content-center">
        <div class="card p-3">
            <div class="d-flex align-items-center" style="border-color:#00ff21;">
                <div class="image"> <img src="https://images.unsplash.com/photo-1522075469751-3a6694fb2f61?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=80" class="rounded" width="155"> </div>
                <div class="ml-3 w-100">
                    <h4 class="mb-0 mt-0">@Html.DisplayFor(model => model.STUDENT_INFO.FIRST_NAME)</h4>
                    <span>@Html.DisplayFor(model => model.PROJECT_NAME)</span><br />
                    <span>@Html.DisplayFor(model => model.PROJECT_MARK)</span>
                    <div class="button mt-2 d-flex flex-row align-items-center">
                        @using (Html.BeginForm())
                        {
                            @Html.AntiForgeryToken()
                            <button class="btn btn-sm btn-outline-primary w-100">Delete</button>
                        }
                        <button class="btn btn-sm btn-primary w-100 ml-2" onclick="@("window.location.href='" + @Url.Action("Details2", "Student",new { id = Model.SPR_id }) + "'");">Back</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script>
        var el = document.getElementsByClassName("btn btn-sm btn-outline-primary w-100");
        el.addEventListener("click", function () { window.location.href = '@Url.Action("Delete", "Student")/'; }, false);
        el.addEventListener("click", function () { window.location.href = '@Html.ActionLink("Back to List", "Students")/'; }, false);
